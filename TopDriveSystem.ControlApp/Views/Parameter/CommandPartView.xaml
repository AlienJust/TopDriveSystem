<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             d:DesignHeight="45" d:DesignWidth="800"
             x:Class="TopDriveSystem.ControlApp.Views.Parameter.CommandPartView">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>
        <TextBox Text="{Binding Path=DataText, Mode=OneWay}" IsReadOnly="True" />
        <TextBlock Grid.Column="1" Text="{Binding Path=ReceiveTimeText, Mode=OneWay}" />
    </Grid>
</UserControl>